msgid "/bin"
msgstr ""

#: devmanual/general-concepts/filesystem/text.xml:12(li)
msgid "<placeholder-1/>: Boot-critical applications"
msgstr ""

#: devmanual/general-concepts/filesystem/text.xml:13(c) devmanual/general-concepts/filesystem/text.xml:61(c)
msgid "/etc"
msgstr ""

#: devmanual/general-concepts/filesystem/text.xml:13(li)
msgid "<placeholder-1/>: System administrator controlled configuration files"
msgstr ""

#: devmanual/general-concepts/filesystem/text.xml:14(c)
msgid "/lib"
msgstr ""

#: devmanual/general-concepts/filesystem/text.xml:14(li)
msgid "<placeholder-1/>: Boot-critical libraries"
msgstr ""

#: devmanual/general-concepts/filesystem/text.xml:15(c) devmanual/general-concepts/filesystem/text.xml:42(c) devmanual/general-concepts/filesystem/text.xml:46(c) devmanual/general-concepts/filesystem/text.xml:47(c)
msgid "/opt"
msgstr ""

#: devmanual/general-concepts/filesystem/text.xml:15(li)
msgid "<placeholder-1/>: Binary-only applications."
msgstr ""

#: devmanual/general-concepts/filesystem/text.xml:16(c)
msgid "/sbin"
msgstr ""

#: devmanual/general-concepts/filesystem/text.xml:16(li)
msgid "<placeholder-1/>: System administrator boot-critical applications"
msgstr ""

#: devmanual/general-concepts/filesystem/text.xml:17(c)
msgid "/tmp"
msgstr ""

#: devmanual/general-concepts/filesystem/text.xml:17(li)
msgid "<placeholder-1/>: Temporary data"
msgstr ""

#: devmanual/general-concepts/filesystem/text.xml:18(c) devmanual/general-concepts/filesystem/text.xml:35(c) devmanual/general-concepts/filesystem/text.xml:41(c) devmanual/general-concepts/filesystem/text.xml:42(c)
msgid "/usr"
msgstr ""

#: devmanual/general-concepts/filesystem/text.xml:18(li)
msgid "<placeholder-1/>: General applications"
msgstr ""

#: devmanual/general-concepts/filesystem/text.xml:20(c)
msgid "/usr/bin"
msgstr ""

#: devmanual/general-concepts/filesystem/text.xml:20(li)
