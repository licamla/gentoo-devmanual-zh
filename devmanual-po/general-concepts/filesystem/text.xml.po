msgid "/usr/local"
msgstr ""

#: devmanual/general-concepts/filesystem/text.xml:22(b)
msgid "Ebuilds must not install here"
msgstr ""

#: devmanual/general-concepts/filesystem/text.xml:22(li)
msgid "<placeholder-1/>: Non-portage applications. <placeholder-2/>."
msgstr ""

#: devmanual/general-concepts/filesystem/text.xml:23(c)
msgid "/usr/sbin"
msgstr ""

#: devmanual/general-concepts/filesystem/text.xml:23(li)
msgid "<placeholder-1/>: Non-system-critical system administrator applications"
msgstr ""

#: devmanual/general-concepts/filesystem/text.xml:24(c) devmanual/general-concepts/filesystem/text.xml:56(c) devmanual/general-concepts/filesystem/text.xml:64(c)
msgid "/usr/share"
msgstr ""

#: devmanual/general-concepts/filesystem/text.xml:24(li)
