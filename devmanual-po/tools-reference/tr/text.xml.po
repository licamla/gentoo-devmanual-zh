#: devmanual/tools-reference/tr/text.xml:4(title)
msgid "tr -- Character Translation"
msgstr ""

#: devmanual/tools-reference/tr/text.xml:8(c) devmanual/tools-reference/tr/text.xml:15(c) devmanual/tools-reference/tr/text.xml:21(c)
msgid "tr"
msgstr ""

#: devmanual/tools-reference/tr/text.xml:9(c)
msgid "man tr"
msgstr ""

#: devmanual/tools-reference/tr/text.xml:10(uri)
msgid "IEEE1003.1-2004-tr"
msgstr ""

#: devmanual/tools-reference/tr/text.xml:7(p)
msgid "The <placeholder-1/> command can be used to translate, squeeze and delete character sequences. See <placeholder-2/> and <placeholder-3/> for the full specification."
msgstr ""

#: devmanual/tools-reference/tr/text.xml:17(c)
msgid "tr [options] &lt; input &gt; output"
msgstr ""

#: devmanual/tools-reference/tr/text.xml:14(note)
msgid "<placeholder-1/>, unlike most other utilities, only reads from standard input and only writes to standard output. Therefore, you will have to use <placeholder-2/>."
msgstr ""

#: devmanual/tools-reference/tr/text.xml:20(p)
msgid "<placeholder-1/> operates in a number of modes, depending upon the invocation:"
msgstr ""

#: devmanual/tools-reference/tr/text.xml:25(dt)
msgid "Deleting characters"
msgstr ""

#: devmanual/tools-reference/tr/text.xml:30(c)
msgid "tr -d asdf"
msgstr ""

#: devmanual/tools-reference/tr/text.xml:29(p)
msgid "To delete all occurrences of certain characters, use <placeholder-1/>."
msgstr ""

#: devmanual/tools-reference/tr/text.xml:33(dt)
msgid "Deleting repeated characters"
msgstr ""

#: devmanual/tools-reference/tr/text.xml:39(c)
msgid "tr -s asdf"
msgstr ""

#: devmanual/tools-reference/tr/text.xml:37(p)
msgid "To replace repeated characters with a single character ('squeeze'), use <placeholder-1/>."
msgstr ""

#: devmanual/tools-reference/tr/text.xml:42(dt)
msgid "Transliterating characters"
msgstr ""

#: devmanual/tools-reference/tr/text.xml:48(c)
msgid "tr abc 123"
msgstr ""

#: devmanual/tools-reference/tr/text.xml:46(p)
msgid "To replace all 'a' characters with '1', all 'b' with '2' and all 'c' with '3', use <placeholder-1/>."
msgstr ""

#: devmanual/tools-reference/tr/text.xml:54(c)
msgid "a-z"
msgstr ""

#: devmanual/tools-reference/tr/text.xml:55(c)
msgid "\\t"
msgstr ""

#: devmanual/tools-reference/tr/text.xml:53(p)
msgid "Certain special forms are allowed for the arguments. <placeholder-1/> expands to all characters from 'a' to 'z', <placeholder-2/> represents a tab and so on. See the documentation for a full list."
msgstr ""

