#: devmanual/ebuild-writing/functions/src_test/text.xml:35(codesample)
msgid "src_test() { if emake -j1 check -n &amp;&gt; /dev/null; then vecho \"&gt;&gt;&gt; Test phase [check]: ${CATEGORY}/${PF}\" if ! emake -j1 check; then hasq test $FEATURES &amp;&amp; die \"Make check failed. See above for details.\" hasq test $FEATURES || eerror \"Make check failed. See above for details.\" fi elif emake -j1 test -n &amp;&gt; /dev/null; then vecho \"&gt;&gt;&gt; Test phase [test]: ${CATEGORY}/${PF}\" if ! emake -j1 test; then hasq test $FEATURES &amp;&amp; die \"Make test failed. See above for details.\" hasq test $FEATURES || eerror \"Make test failed. See above for details.\" fi else vecho \"&gt;&gt;&gt; Test phase [none]: ${CATEGORY}/${PF}\" fi }"
msgstr ""

#: devmanual/ebuild-writing/functions/src_test/text.xml:58(title)
msgid "Sample <c>src_test</c>"
msgstr ""

#: devmanual/ebuild-writing/functions/src_test/text.xml:60(codesample)
msgid "src_test() { cd \"${S}\"/src/testdir # Test 49 won't work inside a portage environment sed -i -e 's~test49.out~~g' Makefile # Try to run the non-gui tests only make test-nongui \\ || die \"At least one test failed\" }"
msgstr ""

