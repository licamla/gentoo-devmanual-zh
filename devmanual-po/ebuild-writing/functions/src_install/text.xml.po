#: devmanual/ebuild-writing/functions/src_install/text.xml:32(title)
msgid "Default <c>src_install</c>"
msgstr ""

#: devmanual/ebuild-writing/functions/src_install/text.xml:34(p)
msgid "For EAPIs 0,1,2, and 3, the default <placeholder-1/> function is the following:"
msgstr ""

#: devmanual/ebuild-writing/functions/src_install/text.xml:38(codesample)
msgid "src_install() { return }"
msgstr ""

#: devmanual/ebuild-writing/functions/src_install/text.xml:44(p)
msgid "For EAPIs 4 and later, the default <placeholder-1/> function is the following:"
msgstr ""

#: devmanual/ebuild-writing/functions/src_install/text.xml:47(codesample)
msgid "src_install() { if [[ -f Makefile ]] || [[ -f GNUmakefile]] || [[ -f makefile ]] ; then emake DESTDIR=\"${D}\" install fi if ! declare -p DOCS &gt;/dev/null 2&gt;&amp;1 ; then local d for d in README* ChangeLog AUTHORS NEWS TODO CHANGES THANKS BUGS \\ FAQ CREDITS CHANGELOG ; do [[ -s \"${d}\" ]] &amp;&amp; dodoc \"${d}\" done elif declare -p DOCS | grep -q \"^declare -a \" ; then dodoc \"${DOCS[@]}\" else dodoc ${DOCS} fi }"
msgstr ""

#: devmanual/ebuild-writing/functions/src_install/text.xml:66(p)
msgid "For EAPIs 6 and later, the default <placeholder-1/> function is the following:"
msgstr ""

#: devmanual/ebuild-writing/functions/src_install/text.xml:69(codesample)
msgid "src_install() { if [[ -f Makefile ]] || [[ -f GNUmakefile ]] || [[ -f makefile ]] ; then emake DESTDIR=\"${D}\" install fi einstalldocs }"
msgstr ""

#: devmanual/ebuild-writing/functions/src_install/text.xml:77(important)
msgid "The following examples assume EAPI 4 or later"
msgstr ""

#: devmanual/ebuild-writing/functions/src_install/text.xml:82(title)
msgid "Sample <c>src_install</c>"
msgstr ""

#: devmanual/ebuild-writing/functions/src_install/text.xml:85(codesample)
msgid "src_install() { emake DESTDIR=\"${D}\" install dodoc README CHANGES }"
msgstr ""

#: devmanual/ebuild-writing/functions/src_install/text.xml:96(title)
msgid "Easy Installs"
msgstr ""

#: devmanual/ebuild-writing/functions/src_install/text.xml:99(c) devmanual/ebuild-writing/functions/src_install/text.xml:142(c) devmanual/ebuild-writing/functions/src_install/text.xml:212(c) devmanual/ebuild-writing/functions/src_install/text.xml:221(c) devmanual/ebuild-writing/functions/src_install/text.xml:223(c)
