msgstr ""

#: devmanual/profiles/use.desc/text.xml:8(c)
msgid "use.desc"
msgstr ""

#: devmanual/profiles/use.desc/text.xml:8(c) devmanual/profiles/use.desc/text.xml:15(c) devmanual/profiles/use.desc/text.xml:26(c)
msgid "USE"
msgstr ""

#: devmanual/profiles/use.desc/text.xml:10(c)
msgid "gentoo-dev"
msgstr ""

#: devmanual/profiles/use.desc/text.xml:7(p)
msgid "The <placeholder-1/> file contains a list of all global non-expanded <placeholder-2/> flags, together with a short description. This file should not be modified without discussion on the <placeholder-3/> list."
msgstr ""

#: devmanual/profiles/use.desc/text.xml:14(c) devmanual/profiles/use.desc/text.xml:16(c) devmanual/profiles/use.desc/text.xml:22(c)
msgid "metadata.xml"
msgstr ""

#: devmanual/profiles/use.desc/text.xml:17(uri)
msgid "here."
msgstr ""

#: devmanual/profiles/use.desc/text.xml:13(p)
msgid "The <placeholder-1/> file on each ebuild category contains a list of the local <placeholder-2/> flags, together with short description for the package in the said category. More information about the <placeholder-3/> file can be found <placeholder-4/>"
msgstr ""

#: devmanual/profiles/use.desc/text.xml:21(c)
msgid "use.local.desc"
msgstr ""

#: devmanual/profiles/use.desc/text.xml:20(note)
msgid "<placeholder-1/> used to be the default location for all the local useflags. However, this file is now an autogenerated aggregation of all the <placeholder-2/> files so you must never edit it directly."
msgstr ""

#: devmanual/profiles/use.desc/text.xml:25(p)
msgid "Having a small number of packages using identically named local <placeholder-1/> flags is allowed. If the number starts to grow substantially, it may be worth proposing that the flag becomes a global <d/> see <uri link=\"::general-concepts/use-flags#Local and Global USE Flags\"/>."
msgstr ""

#: devmanual/profiles/use.desc/text.xml:32(p)
msgid "All non-expand flags must be listed in exactly one of these files."
msgstr ""

#: devmanual/profiles/use.desc/text.xml:36(c)
msgid "desc/${prefix}.desc"
msgstr ""

#: devmanual/profiles/use.desc/text.xml:36(c)
msgid "${prefix}"
msgstr ""

#: devmanual/profiles/use.desc/text.xml:36(p)
msgid "Expanded flags are listed in <placeholder-1/>, where <placeholder-2/> is the name of the environment variable to be expanded in lowercase. Instead of listing the complete flag only the flag's suffix, i.e. the environment variable's possible values are listed."
msgstr ""

